{
    "configurations": [
        {
            "name": "ESP-IDF",
            "includePath": [
                "${workspaceFolder}/**",
                "${env:HOME}/esp-idf/components/**",
                "${env:HOME}/esp-idf/components/esp_common/include",
                "${env:HOME}/esp-idf/components/esp_hw_support/include",
                "${env:HOME}/esp-idf/components/esp_system/include",
                "${env:HOME}/esp-idf/components/freertos/",
                "${env:HOME}/esp-idf/components/driver/",
                "${env:HOME}/esp-idf/components/esp_timer/include",
                "${env:HOME}/esp-idf/components/soc/include",
                "${env:HOME}/esp-idf/components/hal/esp32/include",
                "${env:HOME}/esp-idf/components/log/include",
                "${env:HOME}/esp-idf/components/xtensa/include"
            ],
            "defines": [
                "ESP_PLATFORM",
                "IDF_VER=\"v6.1-dev\"",
                "CONFIG_IDF_TARGET_ESP32"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64",
            "compileCommands": [
                "${workspaceFolder}/build/compile_commands.json"
            ],
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}